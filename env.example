# Vercel Environment Variables
# Copy this file to .env.local and fill in your values

# Vercel KV (Redis) - Get from Vercel Dashboard > Storage > KV
KV_REST_API_URL=
KV_REST_API_TOKEN=
KV_REST_API_READ_ONLY_TOKEN=

# Vercel Postgres - Get from Vercel Dashboard > Storage > Postgres
POSTGRES_URL=
POSTGRES_PRISMA_URL=
POSTGRES_URL_NON_POOLING=
POSTGRES_USER=
POSTGRES_HOST=
POSTGRES_PASSWORD=
POSTGRES_DATABASE=

# Vercel Blob Storage - Get from Vercel Dashboard > Storage > Blob
BLOB_READ_WRITE_TOKEN=

# Custom environment variable
CUSTOM_KEY=your-custom-value

# Analytics (automatically configured by Vercel)
# VERCEL_ANALYTICS_ID is set automatically
